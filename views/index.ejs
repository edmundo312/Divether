<!DOCTYPE html>
<html lang="en">
<head>
    <%- include("template/head", {tittle: "Divether 6", base_url:base_url})  %>    
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <script src="https://code.jquery.com/jquery-2.2.0.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.js"></script>
    </head>
<body>
  <div class="contenedor base_url="<%= base_url %>"">
      <%- include("template/navbar")  %>
      <div id="desceleccionar" class="main">
            <!--hero y boton-->
            <div class="wrapper-contenido">
                <div class="hero">
                    <div class="contenido-hero">
                        <h1>Distribuidora Veterinaria <br> 6 hermanos</h1>
                        <a class="boton" href="tel:5562095444" target="_blank">Llamar Ahora</a>
                    </div>
                </div>
        
                <!--Sobre Nosotros-->
                <h2 class="text-center">Sobre Nosotros</h2>
                <section class="sobre-nosotros">
                  <div class="contenedor sobre-nosotros-grid">
                      <div class="texto-nosotros">
                      <h2>Informacion de la empresa</h2>
                      <p>Empresa dedicada a la venta de productos veterinarios, con mas de 10 años de experiencia
                        en el mercado. Manejando mas de 30 laboratorios, con gran variedad en productos.
                      </p>
                      </div>
                  </div>
                </section>
        
                <!--Laboratorios-->
        
                <div class="container">
                  <h2 class="text-center">Laboratorios</h2>
                   <section class="customer-logos slider">
                      <% laboratorios.forEach(carousel =>{%>
                        <div class="slide"><a href="/laboratorios/laboratorio/<%=carousel.laboratorio_id%>"><img src="/img/<%=carousel.laboratorio_imagen%>"></div></a>
                      <% }) %>
                   </section>
                </div> 
        
                <div>
                  <hr class="linea">
                  <a class="boton-ver-todos" href="/laboratorios">Ver Todos</a>
                </div>
                
                <h2 class="text-center">Promociones</h2>
        
                <section class="section-Promociones contenedor">
                  <div class="promo-grid">
                    <div class="promos-grid ">
                        <img src="/img/laboratorio-chinoin.png" alt="">
                        <div class="boton-pdf">
                          <a href="/pdf/curriculum.pdf">Clic para visualizar archivo</a>
                          <a href="/pdf/curriculum.pdf">Clic para descargar archivo</a>
                        </div>
                    </div>
      
                    <div class="promos-grid ">
                        <img src="/img/laboratorio-Pisa.png" alt="">
                        <div class="boton-pdf">
                          <a href="/pdf/curriculum.pdf">Clic para visualizar archivo</a>
                          <a href="/pdf/curriculum.pdf">Clic para descargar archivo</a>
                        </div>
                    </div>
            
                   
                    <div class="promos-grid ">
                        <img src="/img/laboratorio-diprofaq.png" alt="">
                        <div class="boton-pdf">
                          <a href="/pdf/curriculum.pdf">Clic para visualizar archivo</a>
                          <a href="/pdf/curriculum.pdf">Clic para descargar archivo</a>
                        </div>
                    </div>
                  </div>
                </section>
        
                <hr class="linea">
                <a class="boton-ver-todos"href="/promociones">Ver Todos</a>

               
        
                <h2 class="text-center">Contactanos</h2>
        
                <main class="contenido-principal contenedor">
        
                  <form class="formulario">
                    <fieldset>
                        <legend>Tus datos</legend>
                        
                            <div class="campo">
                                <label for="nombre">Nombre</label>
                                <input id="nombre" type="text" placeholder="Tu Nombre" required>
                            </div>
            
                            <div class="campo">
                                <label for="tel">Telefono</label>
                                <input id="telefono" type="number" placeholder="Tu telefono">
                            </div>
            
                            <div class="campo">
                                <label for="email">Email</label>
                                <input id="email" type="email" placeholder="Tu Email">
                            </div>
        
                            <div class="campo">
                              <label for="asunto">Asunto</label>
                              <input id="asunto" type="text" placeholder="Asunto">
                            </div>
            
                            <div class="campo">
                                <label for="mensaje">Mensaje</label>
                                <textarea></textarea>
                            </div>
        
                            <button class="boton-formulario"type="submit">Enviar Datos</button>
                        </fieldset>                
                </form>
        
                  <div class="contacto">
                    <div class="grid-contacto">
                    
                      <div>
                        <h3>Direccion</h3>
                        <nav class="footer-contacto">
                            <a class="boton" href="https://www.google.com/maps/place/Distribuidora+Veterinaria+6+hermanos/@19.1263164,-98.799493,17z/data=!3m1!4b1!4m5!3m4!1s0x85ce3d24c18bb627:0x3ffcf871c5e8fb2d!8m2!3d19.1263113!4d-98.7973043" target="_blank">Como Llegar</a>
                            <p>Hank Gonzales esqu. con la Viga <br>
                                56900 Ayapango. <br>
                                Estado de México.</p>
                        </nav>
                        
                      </div>
                          
                      <div>
                        <h2>Horario de Atencion</h2>
                        <div class="footer-contacto">
                          <table class="horas">
                            <tbody>
                              <tr>
                                  <th class="dias">lunes:</th>
                                  <td class="horas">
                                      <span class="hora">9:00-17:00</span>
                                  </td>
                              </tr>
                              <tr>
                                  <th class="dias">martes:</th>
                                  <td class="horas">
                                      <span class="hora">9:00-17:00</span>
                                  </td>
                              </tr>
                              <tr>
                                  <th class="dias">miercoles:</th>
                                  <td class="horas">
                                      <span class="hora">9:00-17:00</span>
                                  </td>
                              </tr>
                              <tr>
                                  <th class="dias">jueves:</th>
                                  <td class="horas">
                                      <span class="hora">9:00-17:00</span>
                                  </td>
                              </tr>
                              <tr>
                                  <th class="dias">viernes:</th>
                                  <td class="horas">
                                      <span class="hora">9:00-17:00</span>
                                  </td>
                              </tr>
                              <tr>
                                  <th class="dias">sabado:</th>
                                  <td class="horas">
                                      <span class="hora">9:00-14:00</span>
                                  </td>
                              </tr>
                              <tr>
                                  <th class="dias">domingo:</th>
                                  <td class="horas">
                                      <span class="hora">Cerrado</span>
                                  </td>
                              </tr>
                            </tbody>
                          </table>
                        </div>
                      </div>
                    </div>        
                  </div>
                </main>
              </div>
        
              
      </div>
      <%- include("template/footer")  %>
    </div>
    <script 
        src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" 
        integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" 
        crossorigin="anonymous"
    ></script>
    <%- `<script src="${base_url}js/index.js"></script>`  %>
    <%- `<script src="${base_url}js/template.js"></script>`  %>   
    <%- `<script src="${base_url}js/app.js"></script>`  %> 
    <%- `<script src="${base_url}js/slider.js"></script>`  %> 


    <script>
        posicionarMenu();
         
        $(window).scroll(function() {    
            posicionarMenu();
        });
         
        function posicionarMenu() {
            var altura_del_header = $('.header').outerHeight(true);
            var altura_del_menu = $('.menu-fijo').outerHeight(true);
         
            if ($(window).scrollTop() >= altura_del_header){
                $('.menu-fijo').addClass('fixed');
                $('.wrapper-contenido').css('margin-top', (altura_del_menu) + 'px');
            } else {
                $('.menu-fijo').removeClass('fixed');
                $('.wrapper-contenido').css('margin-top', '0');
            }
        }
         
        </script>     
</body>
</html>